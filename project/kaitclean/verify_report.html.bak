        $("#search_form").submit();
    	}else{
        $("#withdraw").val(F_INSPECT_STATUS);
        $('#search_form').attr('action','');
        $("#search_form").submit();
      }
		}	
   
    function status_to_str(f_field, F_INSPECT_STATUS)
    {
        if (f_field=="F_INSPECT_STATUS" && F_INSPECT_STATUS=="S") return "미검수";
        if (f_field=="F_INSPECT_STATUS" && F_INSPECT_STATUS=="Y") return "완료";
        if (f_field=="F_INSPECT_STATUS" && F_INSPECT_STATUS=="R") return "반려";


        return "";
    }

    function set_search_time(arg_gubun)
    {
        var tmp_time	= '';
        if(arg_gubun == 'start'){
            tmp_time	= $("#f_start_hour option:selected").val() + '' + $("#f_start_minute option:selected").val();
            $('#f_start_time').val(tmp_time);
        } else if(arg_gubun == 'end'){
            tmp_time	= $("#f_end_hour option:selected").val() + '' + $("#f_end_minute option:selected").val();
            $('#f_end_time').val(tmp_time);
        }
    }
    function change_category(level) 
  	{
			
			if(level==2) {
