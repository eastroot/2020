    function deleteData(f_seq, F_INSPECT_STATUS){
        if(!confirm("삭제하시겠습니까?")){
            return;
        }

        $.ajax({
            type:"POST"
            ,async:false
            ,dataType:"json"
            ,data: { "f_seq" : f_seq, "F_INSPECT_STATUS" : F_INSPECT_STATUS}
            ,url:"/pys/changeStatus/"
            ,success:function(args) {
                if (args.status=="OK")
                {
                    alert(args.msg);
                    self.location.reload();
                }
                else
                {
                    alert(args.msg);
                }
            }
            ,beforeSend:null
            ,error: function(e) {
                alert(e.responseText);
            }
        });
    }

	
    function change_status(f_seq, f_field, F_INSPECT_STATUS)
    {
        var str = "";

        str = status_to_str(f_field, F_INSPECT_STATUS);

        if (!confirm(str + "하시겠습니까?"))
            return;
        $('#scroll_pos').val($("#sidebar_contents").scrollTop());
        var f_comment = $('#f_comment_'+f_seq).val();

        $.ajax({
            type:"POST"
            ,async:false
            ,dataType:"json"
            ,data: { "f_seq" : f_seq, "f_field" : f_field, "F_INSPECT_STATUS" : F_INSPECT_STATUS,"f_comment" : f_comment, "csrf_kait_name" : "600535e4fab56f4824fe06c1968a3492"}
            ,url:"/Verify/change_status/"
            ,success:function(args) {
                if (args.status=="OK")
                {
                    alert(args.msg);
                    var pathURL = window.location.pathname ; //부모창의 URL
                    var pathValue = window.location.search; // 부모창의 search 값
                    var fullPath = pathURL + pathValue;
                    if(fullPath.indexOf('?') != -1) {
                        window.location = fullPath + "&scroll_pos=" + $('#scroll_pos').val();
                    }else{
                        window.location = fullPath + "?scroll_pos=" + $('#scroll_pos').val();
                    }
                }
                else
                {
                    alert(args.msg);
                }
            }
            ,beforeSend:null
            ,error: function(e) {
                alert(e.responseText);
            }
            
        });
        self.location.reload();
    }
    
    function tab_change(F_INSPECT_STATUS) {		
    	
			$("#F_INSPECT_STATUS").val(F_INSPECT_STATUS);
