@charset "utf-8";
html, body, blockquote, caption, dd, div, dl, dt, fieldset, form, frame, h1, h2, h3, h4, h5, h6, hr,
iframe, input, legend, li, object, ol, p, pre, q, select, table, textarea, ul, tr, td{ margin:0; padding:0}
body{font-size: 12px; font-family:'Nanum Gothic','나눔고딕', Dotum, "돋움", AppleGothic, Sans-serif; color:#000}
address,em{font-style:normal}
a:hover, a:link,a:visited,a:active{text-decoration:none;}

table{border-collapse:collapse; }

table,fieldset,a,select,input{border:0}
table{font-size:12px;}
img {border:0; vertical-align:middle;}
caption, legend{font-size:0; line-height:0; height:0; visibility:hidden}
article, aside, canvas, details, figcaption, figure, footer, header, hgroup, nav, menu, nav, section {display: block}
ul, ol, li{list-style:none}

input.inputTxt {height:24px; padding:0 5px; margin:0; line-height:24px; border:1px solid #abadb3; vertical-align:middle; }
input.radio, 
input[type=radio] {width:13px !important; height:13px !important; margin:-1px 0 0 0; padding:0; border:0; vertical-align:middle; background:none;}
input.check,
input[type=checkbox] {width:13px !important; height:13px !important; margin:0; padding:0; border:0; vertical-align:middle; background:none;}
select.select {height:26px; line-height:26px; padding:0; margin:0; font-size:14px; color:#555555; font-family:"돋움", Dotum, "굴림", Gulim, "Lucida Grande", Tahoma, Verdana, AppleGothic, sans-serif; vertical-align:middle; border:1px solid #abadb3;}

/* 스킵네비게이션 */
.skip {position:absolute; top:-50px; left:50%; line-height:27px; width:130px; height:40px; padding:0; margin:0 0 0 -50px; text-align:center; overflow:hidden; font-size:12px; line-height:180%; color:#fff; background-color: #333; z-index:100;}
.skip li {list-style-type: none;}
.skip a {width:100px; color:#ffffff; font-weight: bold; }


