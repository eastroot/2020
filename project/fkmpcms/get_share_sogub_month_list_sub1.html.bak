	function member_detail_list(f_personcode, f_bosang_type, f_realname){ 

		$.ajax({
			type:"POST" 
			,async:false
			,dataType:"json" 
			,data: { "f_personcode" : f_personcode, "f_bosang_type" : f_bosang_type, "<?=$this->security->get_csrf_token_name()?>" :"<?=$this->security->get_csrf_hash() ?>"}
			,url:"/share/Share_sogub/insert_sogub_data/"
			,success:function(args) { 
				if (args.status == "ok")
				{ 
					$("#get_share_sogub_month_list_sub2",parent.document).contents().find("#f_personcode").val(args.f_personcode);
					$("#get_share_sogub_month_list_sub2",parent.document).contents().find("#f_bosang_type").val(f_bosang_type);
					$("#get_share_sogub_month_list_sub2", parent.document).get(0).contentWindow.sharelist_search();
				}
				else
					alert(args.msg);
			}
			,beforeSend:null
			,error: function(e) {
				alert(e.responseText);
			}
		});
	}
	
	/**
	* 엑셀다운로드
	* return void
	*/
	function print_excel()
	{
		$("#fm_excel").attr('action', '/share/share_sogub/share_month_member_excel');
		$("#fm_excel").submit();
	}	
	

</script>	

