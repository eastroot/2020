		worksheet.write(row, col+11, val[11])
		worksheet.write(row, col+12, val[12])
		worksheet.write(row, col+13, val[13])
		worksheet.write(row, col+14, val[14])
		worksheet.write(row, col+15, val[15])
		row+=1
	workbook.close()			

#메일 전송 함수 호출	
	ldk_send_mail.send_mail(filename)
#메일 전송 후 파일 삭제
	os.remove(filename)
	sys.exit()				
else:
	print("no rows selected.")
	sys.exit()

dbcurs.close()
dbconn.close()
