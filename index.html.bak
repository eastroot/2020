	          		<pre><code class="python hljs" style="height:500px;">
# -*- coding:utf-8 -*-
import pdfkit
import os
import sys
from PyPDF2 import PdfFileMerger, PdfFileReader

step = sys.argv[1]
req_no = sys.argv[2]
group_info_cnt = int(sys.argv[3])

# print(str(step) + '|' + str(req_no))
tmp_save_dir = '/fkmp/web/cms/pdf/htmltopdf_tmp_files'  # 임시저장 디렉토리
# tmp_url = '/entrust/memship/get_memship_step' + str(step) + '?umode=cms&rno=' + str(req_no)
# pdfkit.from_url(tmp_url, '/fkmp/web/cms/pdf/htmltopdf_tmp_files/out.pdf')
# sys.exit()

list_savedpdf = []
config = pdfkit.configuration(wkhtmltopdf='/usr/local/bin/wkhtmltopdf')
options = {
    'page-size': 'A4',
    'margin-top': '0.02in',
    'margin-right': '0.02in',
    'margin-bottom': '0.02in',
    'margin-left': '0.02in',
    'encoding': "utf-8"

}

#print('ok')
#sys.exit()

if step == '0':
    # 병합을위한 개별 pdf 생성
    for i in [1, 2, 3, 4, 5, 6]:
        tmp_save_path = os.path.join(tmp_save_dir, str(req_no) + '_' + str(i) + '.pdf')
        tmp_url = '/entrust/memship/get_memship_step' + str(i) + '?umode=cms&umode_add=pdf&rno=' + str(
            req_no)
        pdfkit.from_url(tmp_url, tmp_save_path, configuration=config, options=options)
        if not os.path.exists(tmp_save_path):
            print("Error")
            sys.exit()
        else:
            list_savedpdf.append(tmp_save_path)

        if i == 1 and group_info_cnt > 0:
            tmp_save_path = os.path.join(tmp_save_dir, str(req_no) + '_' + str(i) + '_group.pdf')
            pdfkit.from_url(
                '/entrust/memship/open_group_info?umode=cms&umode_add=pdf&rno=' + str(
                    req_no),
                tmp_save_path, configuration=config, options=options)
            if not os.path.exists(tmp_save_path):
                print("Error")
                sys.exit()
            else:
                list_savedpdf.append(tmp_save_path)

    # 개별 pdf 병합
    merger = PdfFileMerger()
    pdf_file = None
    for saved_path in list_savedpdf:
        pdf_file = open(saved_path, 'rb')
        merger.append(PdfFileReader(pdf_file))
        pdf_file.close()
        if os.path.exists(saved_path): os.remove(saved_path)
    merge_file_path = os.path.join(tmp_save_dir, str(req_no) + '.pdf')
    with open(merge_file_path, 'wb') as fout:
        merger.write(fout)
    if not os.path.exists(merge_file_path):
        print("Error")
        sys.exit()
else:
    if step == '1' and group_info_cnt > 0:
        tmp_save_path = os.path.join(tmp_save_dir, str(req_no) + '_' + str(step) + '_tmp.pdf')
        tmp_url = '/entrust/memship/get_memship_step' + str(step) + '?umode=cms&umode_add=pdf&rno=' + str(
            req_no)
        pdfkit.from_url(tmp_url, tmp_save_path, configuration=config, options=options)
        if not os.path.exists(tmp_save_path):
            print("Error")
            sys.exit()
        list_savedpdf.append(tmp_save_path)
        tmp_save_path = os.path.join(tmp_save_dir, str(req_no) + '_' + str(step) + '_group.pdf')
        tml_url2 = '/entrust/memship/open_group_info?umode=cms&umode_add=pdf&rno=' + str(req_no)
        pdfkit.from_url(tml_url2, tmp_save_path, configuration=config, options=options)
        if not os.path.exists(tmp_save_path):
            print("Error")
            sys.exit()
        else:
            list_savedpdf.append(tmp_save_path)

        # 개별 pdf 병합
        merger = PdfFileMerger()
        pdf_file = None
        for saved_path in list_savedpdf:
            pdf_file = open(saved_path, 'rb')
            merger.append(PdfFileReader(pdf_file))
            pdf_file.close()
            if os.path.exists(saved_path): os.remove(saved_path)
        merge_file_path = os.path.join(tmp_save_dir, str(req_no) + '_' + str(step) + '.pdf')
        with open(merge_file_path, 'wb') as fout:
            merger.write(fout)
        if not os.path.exists(merge_file_path):
            print("Error")
            sys.exit()
    else:
        tmp_save_path = os.path.join(tmp_save_dir, str(req_no) + '_' + str(step) + '.pdf')
        tmp_url = '/entrust/memship/get_memship_step' + str(step) + '?umode=cms&umode_add=pdf&rno=' + str(
            req_no)
        pdfkit.from_url(tmp_url, tmp_save_path, configuration=config, options=options)
        if not os.path.exists(tmp_save_path):
            print("Error")
            sys.exit()
print("ok")	          			
