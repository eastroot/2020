import os
import sys
import xlsxwriter
import cx_Oracle
from datetime import datetime, timedelta
from pprint    import pprint
import ldk_send_mail

#오늘 날짜 구하기
now = datetime.now()
today = now.strftime("%Y%m%d")

ORACLE_CONNECT = "(119번 db접속정보)"
dbconn = cx_Oracle.connect(ORACLE_CONNECT)
print("Connected to Oracle: " + dbconn.version)

# 오늘 날짜에 등록 된 정기소급 데이터가 존재하는지 확인
sql = ("""select t1.f_year, f_quarter, t1.f_type, t1.f_personcode, t1.f_personname, t1.f_person_type, t1.f_ckjubu, sum(t1.f_account_first) as hap_account_first, t1.f_personcode_last, t2.f_realname as f_personame_last, t1.f_person_type_last, sum(t1.f_account_send) as hap_account_send, t1.f_procdate, t1.f_procnum
				from t_share_member_history t1
				join t_person t2 on t1.f_personcode_last=t2.f_personcode
				group by t1.f_year, f_quarter, t1.f_type, t1.f_personcode, t1.f_personname, t1.f_person_type, t1.f_ckjubu, t1.f_personcode_last, t2.f_realname, t1.f_person_type_last, t1.f_procdate, t1.f_procnum
				having to_char(t1.f_procdate, 'YYYYMMDD') = '%s'""" % (today))
dbcurs = dbconn.cursor()
dbcurs.execute(sql)
val = dbcurs.fetchone()
if val:
	list=[]
	for row in dbcurs:
# 등록된 정기소급 데이터에 해당하는 로그 개수 및 합산 금액을 115번 db(Log data)에서 조회
		ORACLE_CONNECT2 = "(115번 db접속정보)"
		dbconn2 = cx_Oracle.connect(ORACLE_CONNECT2)

		f_year=row[0]
		f_quarter=row[1]
		f_type=row[2]
		f_personcode=row[3]
		f_personname=row[4]
		f_person_type=row[5]
		f_ckjubu=row[6]
		hap_account_first=row[7]
		f_personcode_last=row[8]
		f_personname_last=row[9]
		f_person_type_last=row[10]
		hap_account_send=row[11]
		f_procdate=row[12]
		f_procnum=str(row[13])
		
		tname='T_SHARELIST_'+f_year+'_'+f_quarter+'_'+f_type
		
		sql2=("select count(1) as row_cnt_115, sum(f_account) as hap_account_115 from "+tname
					+" where f_year='"+f_year 
					+"' and f_quarter='"+f_quarter 
					+"' and f_ckjubu='"+f_ckjubu
					+"' and f_personcd_first='"+f_personcode 
					+"' and f_personcd='"+f_personcode_last 
					+"' and f_procnum="+f_procnum)
		
		dbcurs2 = dbconn2.cursor()
		dbcurs2.execute(sql2)
		
		for row2 in dbcurs2:
			row_cnt_115=row2[0]
			hap_account_115=row2[1]
		
# 리스트 재정의		
		list.append([f_year, f_quarter, f_type, f_personcode, f_personname, f_person_type, f_ckjubu, hap_account_first, f_personcode_last, f_personname_last, f_person_type_last, hap_account_send, str(f_procdate), f_procnum, row_cnt_115, hap_account_115])
# 엑셀 파일 생성		
	filename = today+'_cms.xlsx'
	workbook = xlsxwriter.Workbook(filename)		
	worksheet = workbook.add_worksheet()
	
	worksheet.write(0, 0, '연도')
	worksheet.write(0, 1, '분기')
	worksheet.write(0, 2, '보상타입')
	worksheet.write(0, 3, '회원코드')
	worksheet.write(0, 4, '회원명')
	worksheet.write(0, 5, '회원타입')
	worksheet.write(0, 6, '주/부/지')
	worksheet.write(0, 7, '최초금엑')
	worksheet.write(0, 8, '최종회원코드')
	worksheet.write(0, 9, '최종회원명')
	worksheet.write(0, 10, '최종회원타입')
	worksheet.write(0, 11, '보낸금액')
	worksheet.write(0, 12, '처리날짜')
	worksheet.write(0, 13, '처리번호')
	worksheet.write(0, 14, '로우수')
	worksheet.write(0, 15, '최종금액')
	row = 1
	col = 0
	for val in list:
		worksheet.write(row, col, val[0])			
		worksheet.write(row, col+1, val[1])
		worksheet.write(row, col+2, val[2])
		worksheet.write(row, col+3, val[3])
		worksheet.write(row, col+4, val[4])
		worksheet.write(row, col+5, val[5])
		worksheet.write(row, col+6, val[6])
